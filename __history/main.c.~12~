#pragma hdrstop
#pragma argsused

#include <stdio.h>
#include "common.h"

#ifdef _WIN32
#include <tchar.h>
#else
  typedef char _TCHAR;
  #define _tmain main
#endif

int _tmain(int argc, _TCHAR* argv[]) 
{
	char c;
    int *p;
	SAMPLE(&p, 100);
    TRAVERSING(p, 100);
    _SAMPLE(p);
    scanf("%c", &c);
	return 0;
}
